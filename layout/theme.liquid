<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="author" content="wpOceans">
    <link rel="shortcut icon" type="image/png" href="{{ 'favicon.png' |  asset_url }}">

    {%- if settings.favicon != blank -%}
      <link rel="icon" type="image/png" href="{{ settings.favicon | image_url: width: 32, height: 32 }}">
    {%- endif -%}

    <title>
      {{ page_title }}
      {%- if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif -%}
      {%- if current_page != 1 %} &ndash; Page {{ current_page }}{% endif -%}
      {%- unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless -%}
    </title>

    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    <link href="{{ 'themify-icons.css' |  asset_url }}" rel="stylesheet">
    <link href="{{ 'font-awesome.min.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'flaticon_ecommerce.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'bootstrap.min.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'animate.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'owl.carousel.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'owl.theme.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'slick.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'slick-theme.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'swiper.min.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'owl.transitions.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'jquery.fancybox.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'odometer-theme-default.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'style.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'custom.css' | asset_url }}" rel="stylesheet">

    {{ content_for_header }}
  </head>

  <body>
    <!-- start page-wrapper -->
    <div class="page-wrapper">
      <!-- start preloader -->
      <div class="preloader">
        <div class="vertical-centered-box">
          <div class="content">
            <div class="loader-circle"></div>
            <div class="loader-line-mask">
              <div class="loader-line"></div>
            </div>
            <img src="{{ 'preloader.png' |  asset_url }}" alt="">
          </div>
        </div>
      </div>
      <!-- end preloader -->

      {% section 'header' %}

      {{ content_for_layout }}

      {% section 'footer' %}

      <!-- popup-quickview -->
      <div id="popup-quickview" class="modal fade" tabindex="-1">
        <div class="modal-dialog quickview-dialog">
          <div class="modal-content">
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
              <i
                class="ti-close"
              ></i>
            </button>
            <div class="modal-body">
              <div class="modal-slider-active owl-carousel">
                <div class="product-details product-wrap">
                  <div class="row">
                    <div class="col-lg-7">
                      <div class="product-single-img owl-carousel">
                        <div class="modal-product">
                          <div class="item-wrap">
                            <div class="item-img">
                              <img src="assets/images/modal/img-1.jpg" alt="">
                            </div>
                            <div class="item-img">
                              <img src="assets/images/modal/img-2.jpg" alt="">
                            </div>
                          </div>
                        </div>
                        <div class="modal-product">
                          <div class="item-wrap">
                            <div class="item-img">
                              <img src="assets/images/modal/img-1.jpg" alt="">
                            </div>
                            <div class="item-img">
                              <img src="assets/images/modal/img-2.jpg" alt="">
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="col-lg-5">
                      <div class="product-item">
                        <div class="product-text">
                          <h4><a href="product-single.html">Alejandra</a></h4>
                          <ul class="product-text-sub">
                            <li>One Shoulder Satin Maxi Dress - Black</li>
                          </ul>
                          <span class="price">$160.00</span>
                          <div class="color-name mt-2">
                            <div>
                              <input
                                type="radio"
                                name="color"
                                id="001"
                                class="aggree color1"
                                checked
                              >
                              <span style="margin:5px"></span>
                              <input type="radio" name="color" class="aggree color2">
                              <span style="margin:5px"> </span>
                            </div>
                          </div>
                          <div class="shop-filter-item size">
                            <div class="size-item">
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="11"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="11">xxs</label>
                              </div>
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="12"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="12">Xs</label>
                              </div>
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="13"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="13">X</label>
                              </div>
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="14"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="14">M</label>
                              </div>
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="15"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="15">L</label>
                              </div>
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="16"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="16">XL</label>
                              </div>
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="17"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="17">XXL</label>
                              </div>
                            </div>
                          </div>
                          <div class="update-btn">
                            <a href="#">Update Cart</a>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="product-details product-wrap">
                  <div class="row">
                    <div class="col-lg-7">
                      <div class="product-single-img owl-carousel">
                        <div class="modal-product">
                          <div class="item-wrap">
                            <div class="item-img">
                              <img src="assets/images/modal/img-1.jpg" alt="">
                            </div>
                            <div class="item-img">
                              <img src="assets/images/modal/img-2.jpg" alt="">
                            </div>
                          </div>
                        </div>
                        <div class="modal-product">
                          <div class="item-wrap">
                            <div class="item-img">
                              <img src="assets/images/modal/img-1.jpg" alt="">
                            </div>
                            <div class="item-img">
                              <img src="assets/images/modal/img-2.jpg" alt="">
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="col-lg-5">
                      <div class="product-item">
                        <div class="product-text">
                          <h4><a href="product-single.html">Alejandra</a></h4>
                          <ul class="product-text-sub">
                            <li>One Shoulder Satin Maxi Dress - Black</li>
                          </ul>
                          <span class="price">$160.00</span>
                          <div class="color-name mt-2">
                            <div>
                              <input
                                type="radio"
                                name="color"
                                id="001"
                                class="aggree color1"
                                checked
                              >
                              <span style="margin:5px"></span>
                              <input type="radio" name="color" class="aggree color2">
                              <span style="margin:5px"> </span>
                            </div>
                          </div>
                          <div class="shop-filter-item size">
                            <div class="size-item">
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="11"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="11">xxs</label>
                              </div>
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="12"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="12">Xs</label>
                              </div>
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="13"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="13">X</label>
                              </div>
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="14"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="14">M</label>
                              </div>
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="15"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="15">L</label>
                              </div>
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="16"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="16">XL</label>
                              </div>
                              <div class="button-group">
                                <input
                                  type="radio"
                                  id="17"
                                  name="size"
                                  class="size1"
                                  value="1"
                                >
                                <label class="size-btn" for="17">XXL</label>
                              </div>
                            </div>
                          </div>
                          <div class="update-btn">
                            <a href="#">Update Cart</a>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- popup-quickview -->
      </div>
    </div>

    <script src="{{ 'jquery.min.js' |  asset_url }}" defer="defer"></script>
    <script src="{{ 'bootstrap.bundle.min.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'modernizr.custom.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'jquery.dlmenu.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'jquery-plugin-collection.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'script.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'custom.js' | asset_url }}" defer="defer"></script>
  </body>
</html>
